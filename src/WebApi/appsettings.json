{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=EcomDb;User Id=user;Password=password;"
  },
  "JwtSettings": {
    "SecretKey": "secret-key",
    "Issuer": "OnlineShopApi",
    "Audience": "OnlineShopApiUsers",
    "ExpirationMinutes": 60
  },
  "StripeSettings": {
    "SecretKey": "stripe_secret",
    "PublishableKey": "key",
    "SuccessUrl": "https://localhost/checkout/success?session_id={CHECKOUT_SESSION_ID}",
    "CancelUrl": "https://localhost/checkout/cancel"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5117"
      },
      "Https": {
        "Url": "https://localhost:7132"
      }
    }
  }
}
